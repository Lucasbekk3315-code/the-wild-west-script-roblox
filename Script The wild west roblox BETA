    local newSize = math.clamp(BISON_MIN_TEXT_SIZE + sizeRatio * (BISON_MAX_TEXT_SIZE - BISON_MIN_TEXT_SIZE), BISON_MIN_TEXT_SIZE, BISON_MAX_TEXT_SIZE)
    billboard.Size = UDim2.new(0, newSize, 0, newSize)
